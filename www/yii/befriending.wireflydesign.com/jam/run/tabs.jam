<script>
$(function() {

	// Create a tab dynamically
	var tabs = $( "#rm" ).tabs();
	var ul = tabs.find( "ul" );
	$( "<li><a href='#tab-3'>Dynamic tab</a></li>" ).appendTo( ul );
	$( "<div id='tab-3'>Can generate or remove tabs on the fly</div>" ).appendTo( tabs );
	tabs.tabs( "refresh" );

});
</script>

