<script>
  $(function() {
    $( ".mytabs" ).tabs({
      beforeLoad: function( event, ui ) {
        ui.jqXHR.fail(function() {
          ui.panel.html( "Couldn't load this tab" );
        });
      }
    });
  });
</script>

